<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistroUsuario">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/dim1"
        android:id="@+id/eleccion_usuario"
        android:name="com.example.usuario.rekindlefrontend.EleccionTipoUsuario"
        tools:layout="@layout/fragment_eleccion_tipo_usuario">



    </fragment>



    <RelativeLayout
        android:id="@+id/formulario"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/eleccion_usuario"
        >

    </RelativeLayout>

</RelativeLayout>